  "term_structure": {
    "name": "Term Structure",
    "description": "Futures term structure for CME assets.",
    "source": [
      "Databento",
      "CME"
    ],
    "type": "table",
    "endpoint": "term_structures/get_term_structure",
    "params": [
      {
        "paramName": "asset_class",
        "description": "The asset class to filter choices.",
        "value": "currency",
        "label": "Asset Class",
        "type": "text",
        "options": [
          {
            "label": "Currency",
            "value": "currency"
          },
          {
            "label": "Equity",
            "value": "equity"
          },
          {
            "label": "Interest Rates",
            "value": "interest_rate"
          },
          {
            "label": "Energy",
            "value": "energy"
          },
          {
            "label": "Metals",
            "value": "metals"
          },
          {
            "label": "Agriculture/Commodity",
            "value": "agriculture-commodity"
          }
        ]
      },
      {
        "paramName": "asset",
        "description": "Underlying asset.",
        "label": "Asset",
        "type": "endpoint",
        "optionsEndpoint": "term_structures/get_choices",
        "optionsParams": {
          "asset_type": "$asset_class"
        },
        "style": {
          "popupWidth": 900
        }
      },
      {
        "paramName": "date",
        "description": "The date of the data. If None, the latest available data will be used.",
        "value": null,
        "label": "Date",
        "type": "date"
      }
    ],
    "gridData": {
      "w": 40,
      "h": 15
    },
    "data": {
      "table": {
        "showAll": false,
        "columnsDefs": [
          {
            "field": "expiration",
            "headerName": "Expiry",
            "type": "date"
          },
          {
            "field": "settlement_price",
            "headerName": "Settlement Price",
            "type": "number"
          }
        ]
      }
    },
    "refetchInterval": false
  },
  "continuous_futures_historical": {
